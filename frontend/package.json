    "cypress": "^15.5.0",
    "jasmine-core": "^5.1.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "querystring-es3": "^0.2.1",
    "typescript": "~5.4.2",
    "url": "^0.11.4"
  },
