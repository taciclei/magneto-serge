# Publication Secrets Template
# Copy this file to .env and fill in your actual tokens

# Crates.io (Rust package registry)
# Get your token from https://crates.io/me
CARGO_REGISTRY_TOKEN=your_cargo_token_here

# NPM (JavaScript package registry)
# Get your token from https://www.npmjs.com/settings/YOUR_USERNAME/tokens
NPM_TOKEN=your_npm_token_here

# PyPI (Python package registry)
# Get your token from https://pypi.org/manage/account/token/
PYPI_TOKEN=your_pypi_token_here

# Maven Central (Java/Kotlin package registry)
# Register at https://central.sonatype.org/
OSSRH_USERNAME=your_sonatype_username
OSSRH_PASSWORD=your_sonatype_password

# GPG for Maven signing
# Generate with: gpg --gen-key
# Export with: gpg --armor --export-secret-keys YOUR_KEY_ID
GPG_PRIVATE_KEY=your_gpg_private_key
GPG_PASSPHRASE=your_gpg_passphrase

# Docker Hub
# Get from https://hub.docker.com/settings/security
DOCKER_USERNAME=your_docker_username
DOCKER_PASSWORD=your_docker_token
