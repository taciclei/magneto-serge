{
    "name": "magneto-serge/phpunit",
    "description": "PHPUnit integration for Magnéto-Serge HTTP/WebSocket recording library",
    "type": "library",
    "keywords": ["phpunit", "testing", "http", "websocket", "recording", "replay", "vcr", "magneto-serge"],
    "license": "MIT",
    "authors": [
        {
            "name": "Magnéto-Serge contributors",
            "email": "contact@taciclei.com"
        }
    ],
    "homepage": "https://github.com/taciclei/magneto-serge",
    "support": {
        "issues": "https://github.com/taciclei/magneto-serge/issues",
        "source": "https://github.com/taciclei/magneto-serge"
    },
    "require": {
        "php": "^8.0",
        "phpunit/phpunit": "^9.0 || ^10.0 || ^11.0",
        "magneto-serge/magneto-serge": "^0.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MagnetoSerge\\PHPUnit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MagnetoSerge\\PHPUnit\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
